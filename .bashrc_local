#!/usr/bin/env bash
[[ -f ~/norc ]] && return

. ~/.config/bash/variables.sh

if [[ ! -z "$PS1" ]] && [[ -n "$BASH_VERSION" ]]; then
    for script in "init" "bindkeys" "functions" "aliases"; do
        . ~/.config/bash/${script}.sh
    done
fi

[[ -f ~/.config/bash/stage.sh ]] && . ~/.config/bash/stage.sh
